/boot            emmc              /dev/block/boot
/system          ext4              /dev/block/system
/system_image    emmc              /dev/block/system
/data	         ext4              /dev/block/data
/cache	         ext4              /dev/block/cache
/recovery        emmc              /dev/block/recovery   flags=backup=1

/external_sd     vfat              /dev/block/mmcblk1p1  /dev/block/mmcblk1
/usb-otg         vfat              /dev/block/sda1      /dev/block/sda              flags=removable;storage;display=USB-OTG
